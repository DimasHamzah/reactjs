{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks",
        "cypress"
    ],
    "rules": {
        "react/jsx-filename-extension": [0],
        "import/extensions": "off",
        "no-console": "off",
        "no-alert": "off",
        "no-unused-vars": ["warn", { "varsIgnorePattern": "VARIABLE_NAME"}],
        "no-underscore-dangle": "off"
    }
}
